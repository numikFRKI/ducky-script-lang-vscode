{
    "publisher": "cvbenur",
    "name": "ducky-script-lang-vscode",
    "displayName": "Ducky Script Language for VSCode",
    "description": "A simple syntax highlighting extension to enable support of Ducky Script.",
    "version": "1.0.5",
    "icon": "img/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/cvbenur/ducky-script-lang-vscode"
    },
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "duckyscript",
                "aliases": [
                    "Ducky Script",
                    "duckyscript"
                ],
                "extensions": [
                    ".txt",
                    ".dd"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "duckyscript",
                "scopeName": "source.duckyscript",
                "path": "./syntaxes/duckyscript.tmLanguage.json"
            }
        ]
    }
}
